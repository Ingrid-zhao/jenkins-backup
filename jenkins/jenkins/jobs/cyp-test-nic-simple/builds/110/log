    @pytest.mark.usefixtures("fbefore")
    @pytest.mark.nicAll
    @pytest.mark.nicSilver
    @pytest.mark.nicSimple
    def test_silver_ConnectivityReboot(self):
        sut1_ip = "192.168.10.10"
        sut2_ip = "192.168.10.11"
        self.cn_test_sut.ssh_exec_cmd(self.client2, "ifconfig {} {}".format(self.pkg_generator["pf1"], sut2_ip))
        self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} {}".format(self.sut_conf["pf1"], sut1_ip))
        rs = 0
        cmdip = "ping -c 5 {}\n".format(sut2_ip)
        cmdipv6 = "ping6 -c 5 -I {} {}\n".format(self.sut_conf["pf1"],self.sut2_pf1_ipv6)
        for num in range(2):
            logging.info("-- check sut connectivity by rebooting for {} times --".format(str(num + 1)))
            if num > 0:
                self.cn_test_sut = sshInstance()
                self.client, self.session = self.cn_test_sut.get_ssh_instance(self.sut_conf["host"], self.sut_conf["user"], self.sut_conf["password"], self.sut_conf["port"])
                if self.sut_conf["os"] == "RHEL":
                    self.cn_test_sut.delete_network(self.session, self.sut_conf["pf1"])
                    self.cn_test_sut.add_network(self.session, self.sut_conf["pf1"])
                else:
                    self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} down".format(self.sut_conf["pf1"]))
                    self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} up".format(self.sut_conf["pf1"]))
                self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} {}".format(self.sut_conf["pf1"], sut1_ip))
            output, stderr = self.cn_test_sut.run_ssh_cmd(self.session, cmdip, self.check_str)
            assert 200 == self.cn_test_sut.commond_rs_check(output, "received, 0% packet loss", True)
