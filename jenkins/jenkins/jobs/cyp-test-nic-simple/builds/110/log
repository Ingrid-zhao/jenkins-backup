self = <Tests.testNics.test_nics.Test_Nics object at 0x7f85d5a47a58>

    @pytest.mark.usefixtures("fbefore")
    @pytest.mark.nicAll
    @pytest.mark.nicGold
    @pytest.mark.nicSilver
    @pytest.mark.nicSimple
    @pytest.mark.test
    def test_gold_PI_Networking_SRIOV_CheckDevice(self):
        cmd = "echo 4 > /sys/class/net/{}/device/sriov_numvfs".format(self.sut_conf["pf1"])
        stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, cmd)
        time.sleep(5)
        if stdout or stderr:
            assert 200 == 400
        sut2_ip = "192.168.10.11"
        self.cn_test_sut.ssh_exec_cmd(self.client2, "ifconfig {} {}\n".format(self.pkg_generator["pf1"], sut2_ip))
        port_up_cmd = "ifconfig {} up"
        #manual_ip_cmd = "ifconfig {}{} 192.168.{}.{}"
        manual_ip_cmd = "ifconfig {}{} 192.168.{}.{}"
        ping_ipv4_cmd = "ping -c 5 192.168.{}.{}"
        ping_ipv6_cmd = "ping6 -c 5 -I {}{} {}"
        for i in range(4):
            vf = "v"+str(i)
            ip_part = str(int(sut2_ip.split(".")[3])+i+1)
            vf_name = "{}{}".format(self.sut_conf["pf1"], vf)
            if self.sut_conf["os"] == "RHEL":
                self.cn_test_sut.delete_network(self.session, vf_name)
                self.cn_test_sut.add_network(self.session, vf_name)
            else:
                stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} down".format(vf_name))
                stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} up".format(vf_name))
    
            stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, manual_ip_cmd.format(self.sut_conf["pf1"], vf,sut2_ip.split(".")[2] ,ip_part))
            time.sleep(3)
            if stdout or stderr:
                assert 200 == 400
            stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client2, ping_ipv4_cmd.format(sut2_ip.split(".")[2], ip_part))
            if stdout.find("received, 0% packet loss") == -1:
                assert 200 == 400
            output, err = self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} | grep inet6".format(vf_name))
            if output.find("inet6") == -1:
                self.cn_test_sut.add_network(self.session, vf_name)
            else:
                stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, ping_ipv6_cmd.format(self.sut_conf["pf1"], vf, self.sut2_pf1_ipv6))
            if stdout.find("received, 0% packet loss") == -1:
                logging.info("packet lost during ping.")
