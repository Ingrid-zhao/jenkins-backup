            if stdout or stderr:
                assert 200 == 400
            stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client2, ping_ipv4_cmd.format(sut2_ip.split(".")[2], ip_part))
            if stdout.find("received, 0% packet loss") == -1:
                assert 200 == 400
            output, err = self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} | grep inet6".format(vf_name))
            if output.find("inet6") == -1:
                self.cn_test_sut.add_network(self.session, vf_name)
            else:
                stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, ping_ipv6_cmd.format(self.sut_conf["pf1"], vf, self.sut2_pf1_ipv6))
