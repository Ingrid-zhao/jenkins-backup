        #manual_ip_cmd = "ifconfig {}{} 192.168.{}.{}"
        manual_ip_cmd = "ifconfig {}{} 192.168.{}.{}"
        ping_ipv4_cmd = "ping -c 5 192.168.{}.{}"
        ping_ipv6_cmd = "ping6 -c 5 -I {}{} {}"
        for i in range(4):
            vf = "v"+str(i)
            ip_part = str(int(sut2_ip.split(".")[3])+i+1)
            vf_name = "{}{}".format(self.sut_conf["pf1"], vf)
            if self.sut_conf["os"] == "RHEL":
                self.cn_test_sut.delete_network(self.session, vf_name)
                self.cn_test_sut.add_network(self.session, vf_name)
            else:
                stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} down".format(vf_name))
                stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, "ifconfig {} up".format(vf_name))
    
            stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, manual_ip_cmd.format(self.sut_conf["pf1"], vf,sut2_ip.split(".")[2] ,ip_part))
