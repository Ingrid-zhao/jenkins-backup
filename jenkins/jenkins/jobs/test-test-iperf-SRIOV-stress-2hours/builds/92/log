self = <Tests.testNics.test_network_sriov_stress.Test_Network_SRIOV_Stress object at 0x7f7b9d728710>
vm1_image = 'rhel.img', vm2_image = 'rhel2.img', ip_type = 'ipv6'

    @pytest.mark.usefixtures("fbefore")
    @pytest.mark.nicAll
    @pytest.mark.nicStress
    @pytest.mark.nicIperfTcpSUTGuestStress
    @pytest.mark.SriovVfIperf
    @pytest.mark.parametrize('vm1_image, vm2_image,', vm_image)
    @pytest.mark.parametrize('ip_type', run_type)
    def test_PI_Iperf_TCP_Stress_SUT_Guest(self, vm1_image, vm2_image, ip_type):
        iperf_log_name = "iperf_vm1_sut2_2hours.txt"
        pf_id, status = self.get_pf_id(self.client, self.pf_name)
        # transfer pf_id to 0000\:00\:00.0 format
        if not pf_id:
            assert 200 == status
        else:
            pf_id_format = pf_id.replace(":", "\\:", 2)
            logging.info("format_pf_id is {}".format(pf_id_format))
            # get the number key of pf_id
            pf_id_key = pf_id.split(":")[1]
            logging.info("pf_id_key: {}".format(pf_id_key))
            # assign IP to PF in SUT and sut2
            sut1_pf_ip = "192.168.10.10"
            sut2_pf_ip = "192.168.10.11"
            logging.info("assign sut1 pf {} ip".format(self.pf_name))
            st = self.assign_ip_ehternet(self.session, sut1_pf_ip, self.pf_name)
            if st != 200:
                logging.error("Case Failed: assign sut1 pf {} failed".format(self.pf_name))
                assert 200 == 400, "Case Failed: assign sut1 pf {} failed"
            logging.info("assign sut2 pf {} ip".format(self.pf2_name))
            st = self.assign_ip_ehternet(self.session2, sut2_pf_ip, self.pf2_name)
            if st != 200:
                logging.error("Case Failed: assign sut2 pf {} failed".format(self.pf2_name))
                assert 200 == 400, "Case Failed: assign sut1 pf {} failed"
    
            # virtulize the pf to vfs
            assert 200 == self.virtualize_pf_to_vfs(self.pf_name)
            # get vf ids
            vf_id_list, status = self.get_vf_ids(self.client, pf_id_key)
            vf_id_list = [i.strip() for i in vf_id_list]
            if status == 200:
                # modprobe vfio-pci
                assert 200 == self.modprobe("vfio-pci")
                # bind vf with vfio-pci
                host_dev_id_list, status = self.bind_vf_to_vfio_pci(self.client, vf_id_list)
                assert 200 == status
            else:
                assert 200 == status
            # get vF device id
            logging.info("Start get vf device id in sut1")
            cmd = "lspci -Dnn | grep %s" % (vf_id_list[1])
            logging.info(cmd)
            vf_device_id = ""
            vf_device_id_rs, error = self.ssh_sut.ssh_exec_cmd(self.client, cmd)
            vf_device_id_rs = vf_device_id_rs.strip()
            if vf_device_id_rs:
                st = False
                vf_device_id = None
                for one in vf_device_id_rs.split(" "):
                    one = " " + one + " "
                    if one.find(" [") > -1 and one.find("] ") > -1 and one.find(":") > -1:
                        vf_device_id = one.replace("[", "").replace("]", "").strip()
                if not vf_device_id:
                    logging.error("Case Failed:vf_device_id is not found")
                    assert 200 == 400, "vf_device_id is not found"
                logging.info("Successfully get vf device id: {}".format(vf_device_id))
            else:
                logging.error("Fail to get vf device id.")
                assert 200 == 400
            # set network
            status = self.default_network_isActive(self.client, self.defult_network)
            if status == "no":
                assert 200 == self.start_default_network(self.defult_network)
    
            # start VMs
            logging.info("Start create VM in sut1")
            if self.sut_info["os"] == "UBUNTU":
                assert 200 == self.create_vm(vm1_image, host_dev_id_list[0],  self.vm1_name, "ubuntu20.04", "5900")
                # assert 200 == self.create_vm(vm2_image, host_dev_id_list[1], self.vm2_name, "ubuntu20.04", "5901")
            else:
                assert 200 == self.create_vm(vm1_image, host_dev_id_list[0], self.vm1_name, "rhel8.2", "5900")
                # assert 200 == self.create_vm(vm2_image, host_dev_id_list[1], self.vm2_name, "rhel8.2", "5901")
            # get VM ssh IP
            vm1_ip, status1 = self.get_vm_ip(self.vm1_name)
            # vm2_ip, status2 = self.get_vm_ip(self.vm2_name)
            # ssh VM1 to set VF
            vm1_vf_ip = "192.168.10.12"
            # vm2_vf_ip = "192.168.10.13"
    
            assert 200 == self.on_session_ssh(self.ssh_sut, self.session,
                                              vm1_ip), "Failed to ssh VM. check your password and IP of VM."
            vm1_vf_name, vm1_ipv6, status = self.ssh_vm_set_VF(vm1_ip, vf_device_id, vm1_vf_ip, "1234567890ac")
            self.ssh_sut.run_ssh_cmd(self.session, "exit\n")
            if status != 200:
                logging.error("Case Failed:vm1 vf related settings failed")
                assert 200 == status, "vm1 vf related settings failed"
            logging.info("try to get ipv6 for {}".format(self.pf2_name))
            if self.sut_info["os"] == "RHEL":
                logging.info("getting ipv6 in redhat.")
                self.ssh_sut.delete_network(self.session2, self.pf2_name)
                self.ssh_sut.add_network(self.session2, self.pf2_name)
            else:
                logging.info("getting ipv6 in ubuntu.")
                #self.ssh_sut.run_ssh_cmd(self.session2, "ifconfig {} down\n\n".format(self.pf2_name))
                self.ssh_sut.run_ssh_cmd(self.session2, "ifconfig {} up\n\n".format(self.pf2_name))
            # run iperf -s from sut2
            if ip_type == "ipv4":
                iperf_server_cmd = "nohup iperf -s -f g&\n\n"
            elif ip_type == "ipv6":
                iperf_server_cmd = "nohup iperf -V -s -f g&\n\n"
            logging.info("run cmd {} in SUT2".format(iperf_server_cmd))
            self.ssh_sut.run_ssh_cmd(self.session2, iperf_server_cmd)
            self.ssh_sut.run_ssh_cmd(self.session2, "exit\n")
            #ssh to vm1
            assert 200 == self.on_session_ssh(self.ssh_sut, self.session,
                                              vm1_ip), "Failed to ssh VM. check your password and IP of VM."
            #vm1 iperf from SUT server
            logging.info("Start running iperf client in VM1 and send data to SUT2")
            assert 200 == self.iperf_client(self.run_time, ip_type, sut2_pf_ip, iperf_log_name, self.sut2_pf1_ipv6, vm1_vf_name)
            self.ssh_sut.run_ssh_cmd(self.session, "exit\n")
            # scp iperf log file form vm1 to sut1
            logging.info("scp iperf log files from VM1 to sut1 /tmp")
            self.ssh_sut.run_ssh_cmd(self.session, "scp root@{}:/tmp/*.txt /tmp\n".format(vm1_ip))
            self.ssh_sut.run_ssh_cmd(self.session, "password\n")
            # copy iperf logs from SUT to the controller logdir
            iperf_log_source = os.path.join("/tmp/", iperf_log_name)
            logging.info(iperf_log_source)
            iperf_log_dest = os.path.join(self.case_logdir, iperf_log_name)
            logging.info(iperf_log_dest)
            logging.info("copying files  {} to the controller server {}".format(iperf_log_source, iperf_log_dest))
            self.sftp.get(iperf_log_source, iperf_log_dest)
