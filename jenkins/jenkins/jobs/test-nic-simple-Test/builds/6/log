            ip_part = str(11+i)
            stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, manual_ip_cmd.format(self.sut_conf["pf1"], vf, ip_part))
            if stdout or stderr:
                assert 200 == 400
