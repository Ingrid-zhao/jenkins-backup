        logging.info("run gold_PI_Networking_SRIOV_CheckDevice")
        cmd = "echo 4 > /sys/class/net/{}/device/sriov_numvfs".format(self.sut_conf["connect_switch_sut_pf"])
        stdout, stderr = self.cn_test_sut.ssh_exec_cmd(self.client, cmd)
