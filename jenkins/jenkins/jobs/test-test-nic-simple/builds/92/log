    @pytest.mark.usefixtures("fbefore")
    @pytest.mark.nicSimple
    def test_silver_ConnectivityReboot(self):
        logs = Logger()
        rs = 0
        cmdip = "ping -c 5 {}\n".format(self.dhcp["ipv4"])
        cmdipv6 = "ping6 -c 5 -I {} {}\n".format(self.sut_conf["connect_switch_sut_pf"],self.dhcp["ipv6"])
        for num in range(5):
            cn_test_sut = sshInstance()
            client, session = cn_test_sut.get_ssh_instance(self.sut_conf["host"], self.sut_conf["user"],
                                                           self.sut_conf["password"], self.sut_conf["port"])
            pf_ip = "192.168.{}.{}".format(self.dhcp["ipv4"].split(".")[2],
                                               str(int(self.dhcp["ipv4"].split(".")[3]) + 1))
            cmd = "ifconfig {} {}\n".format(self.sut_conf["pf1"], pf_ip)
            self.cn_test_sut.run_ssh_cmd(session, cmd)
            output, stderr = cn_test_sut.run_ssh_cmd(session, cmdip, self.check_str)
            assert 200 == cn_test_sut.commond_rs_check(output, "received, 0% packet loss", True)
            output, stderr = cn_test_sut.run_ssh_cmd(session, cmdipv6, self.check_str)
