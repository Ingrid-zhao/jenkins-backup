    @pytest.mark.usefixtures("fbefore")
    @pytest.mark.nicSimple
    def test_PI_Networking_InternetProtocolv4v6_CVL(self):
        sut1_ip = "192.168.10.10"
        sut2_ip = "192.168.10.11"
        cmd1 = "ifconfig {} {}".format(self.sut_conf["pf1"], sut1_ip)
        cmd2 = "ifconfig {} {}".format(self.sut2_conf["pf1"], sut2_ip)
    
        stdout1, stderr1 = self.cn_test_sut.ssh_exec_cmd(self.client, cmd1)
        stdout2, stderr2 = self.cn_test_sut.ssh_exec_cmd(self.client2, cmd2)
        if stdout1 or stderr1:
            assert 200==400,"assegin sut1 ip error"
        if stdout2 or stderr2:
            assert  200==400, "assegin sut2 ip error"
