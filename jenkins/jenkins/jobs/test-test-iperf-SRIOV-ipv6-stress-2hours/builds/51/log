    def check_iperf_log(self, ping_time,run_type):
        timeout = int(ping_time) + 60
    
        if run_type == "ipv4":
            filelist = [
                {"filename": "iperf_ipv4_vm1_vm2.txt", "err_message": "iperf vm1 vm2 ipv4 is error", "rs": 400,"st": False},
                {"filename": "iperf_ipv4_vm1_sut2.txt", "err_message": "iperf vm2 sut2 ipv4 is error", "rs": 400,"st": False},
                {"filename": "iperf_ipv4_sut1_sut2.txt", "err_message": "iperf sut1 sut2 ipv4 is error", "rs": 400,"st": False},
            ]
        if run_type == "ipv6":
            filelist = [
                {"filename": "iperf_ipv6_vm1_vm2.txt", "err_message": "iperf vm1 vm2 ipv6 is error", "rs": 400,"st": False},
                {"filename": "iperf_ipv6_vm1_sut2.txt", "err_message": "iperf vm2 sut2 ipv6 is error", "rs": 400,"st": False},
                {"filename": "iperf_ipv6_sut1_sut2.txt", "err_message": "iperf sut1 sut2 ipv6 is error", "rs": 400,"st": False},
            ]
        tm = 0
        while True:
            for dt in filelist:
                st, code = self.check_content_rs(dt["filename"])
                if st:
                    dt["st"] = st
                    dt["rs"] = code
            all_st = True
            for dt in filelist:
                if not dt["st"]:
                    all_st = False
            if all_st:
                break
    
            time.sleep(10)
            tm = tm + 10
            if tm > timeout:
                break
        for dt in filelist:
