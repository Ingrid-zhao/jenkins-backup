    def teardown(self):
        # status = self.remove_vf_from_pf()
        logging.info("removing VMs: {}, {}".format(self.vm1_name, self.vm2_name))
        status = self.remove_vm(self.vm2_name)
        status = self.remove_vm(self.vm1_name)
        cmd = "echo 0 > /sys/class/net/{}/device/sriov_numvfs".format(self.sut_info["pf1"])
        self.ssh_sut.ssh_exec_cmd(self.client, cmd)
        logging.info("try to reset network for {}".format(self.sut_info["pf1"]))
        if self.sut_info["os"] == "RHEL":
            logging.info("try to reset network for redhat...")
            self.ssh_sut.delete_network(self.session, self.sut_info["pf1"])
            self.ssh_sut.add_network(self.session, self.sut_info["pf1"])
        else:
            logging.info("try to reset network for ubuntu...")
            self.ssh_sut.ssh_exec_cmd(self.client, "ifconfig {} 0".format(self.sut_info["pf1"]))
    
