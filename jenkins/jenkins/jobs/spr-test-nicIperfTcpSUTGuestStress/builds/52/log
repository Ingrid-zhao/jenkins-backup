            elif ip_type == "ipv6":
                iperf_server_cmd = "nohup iperf -V -s -f g&\n\n"
            logging.info("run cmd {} in SUT2".format(iperf_server_cmd))
            self.ssh_sut.run_ssh_cmd(self.session2, iperf_server_cmd)
            self.ssh_sut.run_ssh_cmd(self.session2, "exit\n")
